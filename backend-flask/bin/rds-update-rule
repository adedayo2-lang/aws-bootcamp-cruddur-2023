#! /usr/bin/bash


# CYAN='\033[1;36m'
# NO_COLOR='\033[0m'
# LABEL="rds-update-rule"
# printf "${CYsAN}== ${LABEL}${NO_COLOR}\n"


aws ec2 modify-security-group-rules \
    --group-id $DB_SG_ID \
    --security-group-rules "SecurityGroupRuleId= sg-07e687a8e5cacc9ab ,SecurityGroupRule={IpProtocol=tcp,FromPort=5432,ToPort=5432,CidrIpv4=$CODESPACE_IP/32}"