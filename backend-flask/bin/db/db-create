#! /usr/bin/bash

CYAN='\033[1;36m'
NO_COLOR='\033[0m'
LABEL="db-create"
printf "${CYsAN}== ${LABEL}${NO_COLOR}\n"


echo "db-create"

# NO_DB_CONNECTION_URL=$(sed 's/\/cruddur//g' <<<"$CONNECTION_URL")
# psql $NO_DB_CONNECTION_URL -c "CREATE database cruddur;"

NO_DB_CONNECTION_URL=$(sed 's/\/cruddur//g' <<<"$CONNECTION_URL")
echo "NO_DB_CONNECTION_URL: $NO_DB_CONNECTION_URL"
psql $NO_DB_CONNECTION_URL -c "CREATE database cruddur;"


